{
  "require": {
    "slim/slim": "^4.10",
    "php-di/php-di": "6.3.0",
    "slim/psr7": "1.3.0",
    "slim/twig-view": "3.2.0",
    "guzzlehttp/psr7": "^1.6",
    "paquettg/php-html-parser": "^3.1",
    "phpunit/phpunit": ">=9.0",
    "ext-xdebug": "*",
    "ext-mbstring": "*",
    "phpmailer/phpmailer": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "Adm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test/"
    }
  },
  "config": {
    "process-timeout": 0
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public "
  },
  "require-dev": {
    "ext-curl": "*"
  }
}
